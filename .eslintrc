{
  "extends": "airbnb",
  "parser": "babel-eslint",
  
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  },

  "rules": {
    "strict": 0,
    "linebreak-style": "off",
    "comma-dangle": "off",
    "no-unused-vars": ["error", { "args": "none" }],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": 0,
    "arrow-parens": ["error", "as-needed"],
    "consistent-return": "warn",
    "jsx-a11y/anchor-is-valid": [ "error", {
      "components": [ "Link" ],
      "specialLink": [ "to" ]
    }]
  }
}
