{
  "extends": "airbnb",
  "parser": "babel-eslint",
  
  "env": {
    "es6": true,
    "browser": true
  },

  "rules": {
    "strict": 0,
    "linebreak-style": "off",
    "comma-dangle": "warn",
    "no-unused-vars": ["error", { "args": "none" }],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": 0,
    "arrow-parens": ["error", "as-needed"],
    "consistent-return": "warn"
  }
}
